<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
    <style>
        @import url(http://fonts.googleapis.com/earlyaccess/nanumgothic.css);
        .custom-text, th, td, h1, h3, p { font-family:"Nanum Gothic";}
        .custom-text {
            padding: 12px;
            width:50%;
            position: relative;
            left:50%;
            transform: translate(-50%,0%);
        }
    </style>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
</head>
<body>
    <div class="container">
        <p class="text-primary custom-text">helloworld</p>
        <p class="text-success custom-text">helloworld</p>
        <p class="text-info custom-text">helloworld</p>
        <p class="text-warning custom-text">helloworld</p>
        <p class="text-danger custom-text">helloworld</p>

        <p class="bg-primary custom-text">Hello World</p>
        <p class="bg-success custom-text">Hello World</p>
        <p class="bg-info custom-text">Hello World</p>
        <p class="bg-warning custom-text">Hello World</p>
        <p class="bg-danger custom-text">Hello World</p>
    </div>

</body>
</html>
